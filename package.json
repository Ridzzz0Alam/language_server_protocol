{
  "name": "cc-lsp",
  "displayName": "Coding Challenge LSP",
  "description": "A simple custom Language Server built in Python",
  "version": "0.0.1",
  "publisher": "yourname",
  "engines": {
    "vscode": "^1.50.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "onLanguage:plaintext"
  ],
  "main": "./extension.js",
  "contributes": {
    "languages": [
      {
        "id": "plaintext",
        "aliases": [
          "Plaintext",
          "plain text"
        ],
        "extensions": [
          ".txt"
        ]
      }
    ]
  },
  "dependencies": {
    "vscode-languageclient": "^9.0.1"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
